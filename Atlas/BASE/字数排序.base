formulas:
  Untitled: ""
views:
  - type: table
    name: Table
    filters:
      and:
        - file.ext == "md"
    order:
      - file.name
      - file.size
      - file.folder
      - file.mtime
    sort:
      - property: file.size
        direction: DESC
      - property: file.mtime
        direction: ASC
