formulas:
  Untitled: ""
views:
  - type: table
    name: Table
    filters:
      and:
        - file.ext == "md"
        - file.folder == "5.Misc/copilot-custom-prompts"
    order:
      - file.name
      - file.size
      - file.folder
    sort:
      - property: file.size
        direction: DESC
